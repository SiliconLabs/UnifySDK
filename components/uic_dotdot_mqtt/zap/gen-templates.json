{
    "name": "Unify Dotdot Mappers",
    "version": "Unify-dotdot-v1",
    "override": "../../uic_dotdot/zap/addon-override.js",
    "helpers": [
        "../../uic_dotdot/zap/addon-helper.js"
    ],
    "options": {
        "types": {
            "CSKey":"uint8_t"
        }
    },
    "partials":[{
        "name": "zcl_type",
        "path": "zcl_type_partial.zapt"
    }],
    "templates": [{
            "path": "dotdot_mqtt_cpp.zapt",
            "name": "Implementation for parsing mqtt topics for Unify",
            "output": "src/dotdot_mqtt.cpp"
        },
        {
            "path": "dotdot_mqtt_translators_h.zapt",
            "name": "Templates for custom translators for PTree",
            "output": "include/dotdot_mqtt_translators.h"
        },
        {
            "path": "dotdot_mqtt_h.zapt",
            "name": "Code for parsing mqtt topics for Unify",
            "output": "include/dotdot_mqtt.h"
        },
        {
            "path": "dotdot_mqtt_helpers_hpp.zapt",
            "name": "Dotdot MQTT Helpers interface",
            "output": "include/dotdot_mqtt_helpers.hpp"
        },
        {
            "path": "dotdot_mqtt_helpers_h.zapt",
            "name": "Dotdot MQTT Helpers C interface",
            "output": "include/dotdot_mqtt_helpers.h"
        },
        {
            "path": "dotdot_mqtt_helpers_cpp.zapt",
            "name": "Dotdot MQTT Helpers implementation",
            "output": "src/dotdot_mqtt_helpers.cpp"
        },
        {
            "path": "dotdot_mqtt_hpp.zapt",
            "name": "Internal C++ constants and definitions for Dotdot MQTT",
            "output": "src/dotdot_mqtt.hpp"
        },
        {
            "path": "dotdot_mqtt_attributes_h.zapt",
            "name": "Definitions for parsing MQTT Attributes topics for Unify",
            "output": "include/dotdot_mqtt_attributes.h"
        },
        {
            "path": "dotdot_mqtt_attributes_cpp.zapt",
            "name": "Implementation for parsing MQTT Attributes topics for Unify",
            "output": "src/dotdot_mqtt_attributes.cpp"
        },
        {
            "path": "dotdot_mqtt_command_helpers_hpp.zapt",
            "name": "Definitions for private Commands parsing helpers",
            "output": "src/dotdot_mqtt_command_helpers.hpp"
        },
        {
            "path": "dotdot_mqtt_command_helpers_cpp.zapt",
            "name": "Implementation of private Commands parsing helpers",
            "output": "src/dotdot_mqtt_command_helpers.cpp"
        },
        {
            "path": "dotdot_mqtt_group_commands_h.zapt",
            "name": "Definitions for receiving Cluster Commands over MQTT",
            "output": "include/dotdot_mqtt_group_commands.h"
        },
        {
            "path": "dotdot_mqtt_group_commands_cpp.zapt",
            "name": "Definitions for receiving Cluster Commands over MQTT",
            "output": "src/dotdot_mqtt_group_commands.cpp"
        },
        {
            "path": "dotdot_mqtt_send_commands_h.zapt",
            "name": "Definitions for sending Cluster Commands over MQTT",
            "output": "include/dotdot_mqtt_send_commands.h"
        },
        {
            "path": "dotdot_mqtt_send_commands_cpp.zapt",
            "name": "Implementation for sending Cluster Commands over MQTT",
            "output": "src/dotdot_mqtt_send_commands.cpp"
        },
        {
            "path": "dotdot_mqtt_supported_generated_commands_h.zapt",
            "name": "Definitions for advertising Supported Generated Commands over MQTT",
            "output": "include/dotdot_mqtt_supported_generated_commands.h"
        },
        {
            "path": "dotdot_mqtt_supported_generated_commands_cpp.zapt",
            "name": "Implementation for advertising Supported Generated Commands over MQTT",
            "output": "src/dotdot_mqtt_supported_generated_commands.cpp"
        },
        {
            "path": "dotdot_mqtt_generated_commands_h.zapt",
            "name": "Definitions for publishing Generated Commands over MQTT",
            "output": "include/dotdot_mqtt_generated_commands.h"
        },
        {
            "path": "dotdot_mqtt_generated_commands_cpp.zapt",
            "name": "Implementation for publishing Generated Commands over MQTT",
            "output": "src/dotdot_mqtt_generated_commands.cpp"
        },
        {
            "path": "../test/dotdot_mqtt_test.include.zapt",
            "name": "Unittest for dotdot_mqtt",
            "output": "test/dotdot_mqtt_test.include"
        }
    ]
}