def zbCARBON_MONOXIDE_MIN_MEASURED_VALUE 0x040c0001
def zbCARBON_MONOXIDE_MAX_MEASURED_VALUE 0x040c0002
def zbCARBON_MONOXIDE_TOLERANCE          0x040c0003

scope 0 chain_reaction(0) {
  r'zbCARBON_MONOXIDE_MIN_MEASURED_VALUE = if(e'zbCARBON_MONOXIDE_MIN_MEASURED_VALUE) 0 undefined

  r'zbCARBON_MONOXIDE_MAX_MEASURED_VALUE = if(e'zbCARBON_MONOXIDE_MAX_MEASURED_VALUE) 1 undefined

  r'zbCARBON_MONOXIDE_TOLERANCE = if(e'zbCARBON_MONOXIDE_TOLERANCE) 50 undefined
}
