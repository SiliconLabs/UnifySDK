// ZCL attributes for pH measurement. It will be in the range [0;1400] representing [0;14]
def zbPH_MEASUREMENT_MEASURED_VALUE     0x04090000
def zbPH_MEASUREMENT_MIN_MEASURED_VALUE 0x04090001
def zbPH_MEASUREMENT_MAX_MEASURED_VALUE 0x04090002
def zbPH_MEASUREMENT_TOLERANCE          0x04090003

scope 0 chain_reaction(0) {

  r'zbPH_MEASUREMENT_MIN_MEASURED_VALUE = if (e'zbPH_MEASUREMENT_MIN_MEASURED_VALUE) 0 undefined

  r'zbPH_MEASUREMENT_MAX_MEASURED_VALUE = if (e'zbPH_MEASUREMENT_MAX_MEASURED_VALUE) 1400 undefined
    
  r'zbPH_MEASUREMENT_TOLERANCE = if (e'zbPH_MEASUREMENT_TOLERANCE) 10 undefined
}
