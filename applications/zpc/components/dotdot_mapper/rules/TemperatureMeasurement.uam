
//Sensor multilevel CC
def zwSENSOR_MULTILEVEL_SENSOR_TYPE  0x3102
def zwSENSOR_MULTILEVEL_SCALE        0x3104
def zwSENSOR_MULTILEVEL_SENSOR_VALUE 0x3105 // Value is int32 with 3 decimal points, i.e. 1000 means 1

def ZW_SENSOR_TYPE_AIR_TEMPERATURE 0x01

//the DotDot TemperatureMeasurement cluster
def DOTDOT_ATTRIBUTE_ID_TEMPERATURE_MEASUREMENT_MEASURED_VALUE 0x04020000
def DOTDOT_ATTRIBUTE_ID_TEMPERATURE_MEASUREMENT_MIN_MEASURED_VALUE 0x04020001
def DOTDOT_ATTRIBUTE_ID_TEMPERATURE_MEASUREMENT_MAX_MEASURED_VALUE 0x04020002
def DOTDOT_ATTRIBUTE_ID_TEMPERATURE_MEASUREMENT_TOLERANCE 0x04020003

scope 0 {

r'DOTDOT_ATTRIBUTE_ID_TEMPERATURE_MEASUREMENT_MEASURED_VALUE =
  if(r'zwSENSOR_MULTILEVEL_SENSOR_TYPE[ZW_SENSOR_TYPE_AIR_TEMPERATURE].zwSENSOR_MULTILEVEL_SCALE == 0)
    (r'zwSENSOR_MULTILEVEL_SENSOR_TYPE[ZW_SENSOR_TYPE_AIR_TEMPERATURE].zwSENSOR_MULTILEVEL_SENSOR_VALUE  / 10)
  if(r'zwSENSOR_MULTILEVEL_SENSOR_TYPE[ZW_SENSOR_TYPE_AIR_TEMPERATURE].zwSENSOR_MULTILEVEL_SCALE == 1)
    (((r'zwSENSOR_MULTILEVEL_SENSOR_TYPE[ZW_SENSOR_TYPE_AIR_TEMPERATURE].zwSENSOR_MULTILEVEL_SENSOR_VALUE - 32000) * 5) / 90)
    undefined
}
